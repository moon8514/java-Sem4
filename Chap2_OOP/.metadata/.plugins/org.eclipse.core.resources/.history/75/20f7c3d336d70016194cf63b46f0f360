package bai4_Triangle;

public class Triangle {
	float a,b,c;

	public Triangle() {
		a = b = c = 0f;
	}
	
	public Triangle(float a, float b, float c) {
		this.a = a;
		this.b = b;
		this.c = c;
	}
	
	public boolean isTriangle() {
		if(a + b > c && a + c > b && b + c > a) return true;
		else return false;
	}
	
}
