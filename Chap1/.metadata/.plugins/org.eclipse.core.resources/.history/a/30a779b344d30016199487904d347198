
public class SortArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[] = {1,4,2,5,9,7,3};
		
		for(int i = 0; i<a.length-1;i++)
		{
			for(int j = i + 1; j<a.length; j++)
			{
				if(a[i] > a[j])
				{
					int t = a[i];
					a[i] = a[j];
					a[i] = t;
				}
			}
		}
		
	}

}
